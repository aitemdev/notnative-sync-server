{
   "name": "notnative-sync-server",
   "version": "1.0.0",
   "description": "NotNative Electron - VPS Sync Server",
   "main": "dist/index.js",
   "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && cp src/services/transformers-bridge.cjs dist/services/",
    "start": "node dist/index.js",
    "migrate": "tsx src/utils/migrate.ts",
    "migrate:add-deleted-at": "tsx src/utils/add-deleted-at.ts",
    "cleanup": "tsx src/utils/cleanup-orphans.ts"
  },
  "keywords": [
    "notnative",
    "sync",
    "notes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.1",
    "tsx": "^4.21.0",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/ws": "^8.5.13",
    "typescript": "^5.7.2"
  }
}
