{
  "name": "notnative-electron",
  "version": "1.0.0",
  "description": "NotNative - Personal Knowledge Management with AI",
  "homepage": "https://github.com/your-username/notnative",
  "author": {
    "name": "NotNative Team",
    "email": "notnative@example.com"
  },
  "main": "dist/main/index.js",
  "type": "module",
  "scripts": {
    "dev": "node scripts/build-preload.mjs && vite",
    "build": "node scripts/build-preload.mjs && tsc && vite build && electron-builder",
    "build:preload": "node scripts/build-preload.mjs",
    "preview": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.0.8",
    "@codemirror/autocomplete": "^6.18.3",
    "@codemirror/commands": "^6.7.1",
    "@codemirror/lang-markdown": "^6.3.1",
    "@codemirror/language": "^6.10.6",
    "@codemirror/language-data": "^6.5.2",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.35.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^9.0.0",
    "@lezer/highlight": "^1.2.3",
    "@openrouter/ai-sdk-provider": "^1.4.1",
    "@replit/codemirror-vim": "^6.2.1",
    "@tanstack/react-table": "^8.20.6",
    "ai": "^5.0.108",
    "better-sqlite3": "^11.6.0",
    "chokidar": "^4.0.2",
    "codemirror": "^6.0.1",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "gray-matter": "^4.0.3",
    "howler": "^2.2.4",
    "hyperformula": "^2.7.1",
    "i18next": "^24.0.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.460.0",
    "marked": "^15.0.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.1.1",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.26.2",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/chokidar": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/d3": "^7.4.3",
    "@types/express": "^5.0.0",
    "@types/howler": "^2.2.12",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "esbuild": "^0.27.1",
    "eslint": "^9.16.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^6.0.1",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "build": {
    "appId": "com.notnative.app",
    "productName": "NotNative",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Office"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    }
  }
}
